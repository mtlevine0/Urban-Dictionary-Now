{
	"manifest_version": 3,
	"name": "Urban Dictionary Now",
	"version":"0.2.0",
	"description": "Allows a user to easily search Urban Dictionary with one click.",
	"icons": {
		"48": "icon48.png",
		"128": "icon96.png"
	},
	"author": "Matt Levine",
	"offline_enabled": false,
	"minimum_chrome_version": "88",
	"permissions": [
		"contextMenus"
	],
	"background": {
		"service_worker": "bundle.js",
		"type": "module"
	}
}
